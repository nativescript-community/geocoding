{
    "name": "@nativescript-community/geocoding",
    "version": "1.0.0",
    "homepage": "https://github.com/nativescript-community/geocoding#readme",
    "bugs": {
        "url": "https://github.com/nativescript-community/geocoding/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nativescript-community/geocoding.git"
    },
    "license": "ISC",
    "author": {
        "name": "Tim Doege",
        "email": "timdoege@gmail.com"
    },
    "scripts": {
        "build": "rm -f .tsbuildinfo && npm run tsc",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "demo.ng.android": "cd ./demo-ng && ns run android --no-hmr",
        "demo.ng.android.dev": "cd ./demo-ng && ns run android --no-hmr --env.development",
        "demo.ng.ios": "cd ./demo-ng && ns run ios --no-hmr",
        "demo.ng.ios.dev": "cd ./demo-ng && ns run ios --no-hmr --env.development",
        "demo.react.android": "npm run build && cd ./demo-react && ns run android --no-hmr",
        "demo.react.android.dev": "npm run build && cd ./demo-react && ns run android --no-hmr --env.development",
        "demo.react.ios": "npm run build && cd ./demo-react && ns run ios --no-hmr",
        "demo.react.ios.dev": "npm run build && cd ./demo-react && ns run ios --no-hmr --env.development",
        "demo.setup": "node ./tools/setup-demos.js",
        "demo.svelte.android": "npm run build && cd ./demo-svelte && ns run android --no-hmr",
        "demo.svelte.android.dev": "npm run build && cd ./demo-svelte && ns run android --no-hmr --env.development",
        "demo.svelte.ios": "npm run build && cd ./demo-svelte && ns run ios --no-hmr",
        "demo.svelte.ios.dev": "npm run build && cd ./demo-svelte && ns run ios --no-hmr --env.development",
        "demo.vue.android": "npm run build && cd ./demo-vue && ns run android --no-hmr",
        "demo.vue.android.dev": "npm run build && cd ./demo-vue && ns run android --no-hmr --env.development",
        "demo.vue.ios": "npm run build && cd ./demo-vue && ns run ios --no-hmr",
        "demo.vue.ios.dev": "npm run build && cd ./demo-vue && ns run ios --no-hmr --env.development",
        "doc": "typedoc --out docs --tsconfig tsconfig.doc.json --includeDeclarations --excludeExternals --mode file --ignoreCompilerErrors --excludeNotExported --excludePrivate --theme minimal",
        "publish": "npm run setup && npm run build && lerna publish --create-release=github --force-publish",
        "setup": "ts-patch install",
        "sync": "node ./tools/sync.js -w && npx sort-package-json",
        "sync.test": "node ./tools/sync.js",
        "tsc": "cpy '**/*.d.ts' '../plugin' --parents --cwd=src && tsc -skipLibCheck -d"
    },
    "dependencies": {
        "json-merge": "^1.2.0",
        "ts-node": "^8.10.2"
    },
    "devDependencies": {
        "@angular/animations": "~11.0.9",
        "@angular/common": "~11.0.9",
        "@angular/compiler": "~11.0.9",
        "@angular/compiler-cli": "~11.0.9",
        "@angular/core": "~11.0.9",
        "@angular/forms": "~11.0.9",
        "@angular/platform-browser": "~11.0.9",
        "@angular/platform-browser-dynamic": "~11.0.9",
        "@angular/router": "~11.0.9",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@nativescript/angular": "11.0.0",
        "@nativescript/core": "7.1.2",
        "@nativescript/ios": "7.1.0",
        "@nativescript/types-android": "~7.1.0",
        "@nativescript/types-ios": "~7.1.0",
        "@nativescript/webpack": "4.0.1",
        "@types/node": "14.14.7",
        "@types/react": "16.9.56",
        "@typescript-eslint/eslint-plugin": "4.7.0",
        "@typescript-eslint/parser": "4.7.0",
        "cpy-cli": "^3.1.1",
        "eslint": "7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.4",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-cli": "^26.6.3",
        "lerna": "^3.22.1",
        "lint-staged": "^10.5.3",
        "ng-packagr": "~10.1.2",
        "npm-deps": "^0.3.7",
        "package-json-merge": "0.0.1",
        "prettier": "^2.1.2",
        "prompt": "^1.0.0",
        "react-nativescript": "^2.2.0",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "svelte-native": "0.9.5",
        "ts-patch": "^1.3.1",
        "tslib": "2.0.3",
        "typedoc": "^0.19.2",
        "typescript": "4.0.5",
        "vue": "^2.6.12",
        "zone.js": "0.11.1"
    }
}